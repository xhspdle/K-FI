<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kfi.mybatis.jyi.CommBoardViewMapper">

	<select id="select_Community" parameterType="hashmap" resultType="commboardview">
		select * from comm_board_view where comm_num=#{comm_num} 
		<if test="cb_num!='' and cb_num!=null">
			and cb_num=#{cb_num}
		</if>
	</select>

</mapper>